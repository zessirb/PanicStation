{

  "manifest_version": 2,
  "name": "Borderify",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icon/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.pole-emploi.fr/*"],
      "js": ["borderify.js"]
    }
  ]

}